
@{
    ViewData["Title"] = "Order History";
}


@section Styles {

}

<!-- Container-fluid starts-->
<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <div class="card">
                <div class="card-header pb-0">
                    <h5>Order History</h5>
                </div>
                <div class="card-body">
                    <div>
                        <table id="orderHistoryTable" class="display" width="100%">
                            <thead>
                                <tr>
                                    <th>Order No</th>
                                    <th>Placed Date</th>
                                    <th>Delivery Date</th>
                                    <th>Payment Method</th>
                                    <th>Total</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Container-fluid Ends -->


@section Scripts {
    <script src="~/scripts/EStore/user-order-history.js" asp-append-version="true"></script>
}